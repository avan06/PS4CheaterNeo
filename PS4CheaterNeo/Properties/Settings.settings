    </Setting>
    <Setting Name="QuerySectionViewFilterSizeForeColor" Type="OptionTreeView.Option&lt;System.Drawing.Color&gt;" Scope="User">
      <Value Profile="(Default)">DarkCyan|5_UI|3_Color Query|Determine the Query SectionView FilterSize foreground color</Value>
    </Setting>
    <Setting Name="QuerySectionViewFilterSizeBackColor" Type="OptionTreeView.Option&lt;System.Drawing.Color&gt;" Scope="User">
      <Value Profile="(Default)">DarkSlateGray|5_UI|3_Color Query|Determine the Query SectionView FilterSize background color</Value>
