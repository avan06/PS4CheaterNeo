    </Setting>
    <Setting Name="QuerySectionViewFilterForeColor" Type="OptionTreeView.Option&lt;System.Drawing.Color&gt;" Scope="User">
      <Value Profile="(Default)">DarkGray|5_UI|3_Color Query|Determine the Query SectionView Filter foreground color</Value>
    </Setting>
    <Setting Name="QuerySectionViewFilterBackColor" Type="OptionTreeView.Option&lt;System.Drawing.Color&gt;" Scope="User">
