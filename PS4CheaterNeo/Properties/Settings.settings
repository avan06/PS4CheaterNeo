    </Setting>
    <Setting Name="MainCheatGridViewGridColor" Type="OptionTreeView.Option&lt;System.Drawing.Color&gt;" Scope="User">
      <Value Profile="(Default)">Silver|5_UI|2_Color Main|Determine the Main CheatGridView grid color</Value>
    </Setting>
    <Setting Name="MainCheatGridCellForeColor" Type="OptionTreeView.Option&lt;System.Drawing.Color&gt;" Scope="User">
      <Value Profile="(Default)">White|5_UI|2_Color Main|Determine the Main dataGridViewCellStyle foreground color</Value>
    </Setting>
    <Setting Name="MainCheatGridCellBackColor" Type="OptionTreeView.Option&lt;System.Drawing.Color&gt;" Scope="User">
