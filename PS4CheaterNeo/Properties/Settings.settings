    <Setting Name="MainToolStrip1BackColor" Type="OptionTreeView.Option&lt;System.Drawing.Color&gt;" Scope="User">
      <Value Profile="(Default)">FF99B4D1|5_UI|2_Color Main|Determine the Main ToolStrip1 background color</Value>
